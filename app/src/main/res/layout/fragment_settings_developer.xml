<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/transparent"
    tools:context=".settings.DeveloperSettingsModule">

    <!-- 设置内容 -->
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingHorizontal="16dp"
            android:paddingBottom="32dp">

            <!-- 杂项 -->
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/miscellaneous"
                android:textColor="?attr/textPrimary"
                android:textSize="16sp"
                android:textStyle="bold"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="12dp" />

            <!-- Force Reinstall Built-in Patches Button -->
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/forceReinstallPatchesCard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="12dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="2dp"
                app:strokeWidth="0.5dp"
                app:strokeColor="?attr/colorOutlineVariant"
                app:cardBackgroundColor="?attr/colorSurfaceContainer"
                style="@style/Widget.App.CardView.Standard">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingHorizontal="16dp"
                    android:paddingVertical="14dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center_vertical">

                        <ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:src="@drawable/ic_refresh"
                            app:tint="?attr/textPrimary"
                            android:layout_marginEnd="16dp"
                            android:contentDescription="@string/force_reinstall_patches" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/force_reinstall_patches"
                                android:textColor="?attr/textPrimary"
                                android:textSize="15sp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/force_reinstall_patches_desc"
                                android:textColor="?attr/textSecondary"
                                android:textSize="12sp"
                                android:layout_marginTop="2dp" />

                        </LinearLayout>

                        <com.google.android.material.button.MaterialButton
                            android:id="@+id/btnForceReinstallPatches"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/reinstall"
                            android:layout_marginStart="16dp"
                            style="@style/Widget.Material3.Button.TonalButton" />

                    </LinearLayout>

                </LinearLayout>

            </com.google.android.material.card.MaterialCardView>

            <!-- CoreCLR 运行时配置标题 -->
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/coreclr_settings"
                android:textColor="?attr/textPrimary"
                android:textSize="16sp"
                android:textStyle="bold"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="12dp" />

            <!-- Server GC -->
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/serverGCCard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="12dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="2dp"
                app:strokeWidth="0.5dp"
                app:strokeColor="?attr/colorOutlineVariant"
                app:cardBackgroundColor="?attr/colorSurfaceContainer"
                style="@style/Widget.App.CardView.Standard">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingHorizontal="16dp"
                    android:paddingVertical="14dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center_vertical">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/coreclr_server_gc"
                                android:textColor="?attr/textPrimary"
                                android:textSize="15sp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/coreclr_server_gc_desc"
                                android:textColor="?attr/textSecondary"
                                android:textSize="12sp"
                                android:layout_marginTop="2dp" />

                        </LinearLayout>

                        <com.google.android.material.materialswitch.MaterialSwitch
                            android:id="@+id/switchServerGC"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="16dp" />

                    </LinearLayout>
                </LinearLayout>
            </com.google.android.material.card.MaterialCardView>

            <!-- Concurrent GC -->
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/concurrentGCCard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="12dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="2dp"
                app:strokeWidth="0.5dp"
                app:strokeColor="?attr/colorOutlineVariant"
                app:cardBackgroundColor="?attr/colorSurfaceContainer"
                style="@style/Widget.App.CardView.Standard">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingHorizontal="16dp"
                    android:paddingVertical="14dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center_vertical">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/coreclr_concurrent_gc"
                                android:textColor="?attr/textPrimary"
                                android:textSize="15sp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/coreclr_concurrent_gc_desc"
                                android:textColor="?attr/textSecondary"
                                android:textSize="12sp"
                                android:layout_marginTop="2dp" />

                        </LinearLayout>

                        <com.google.android.material.materialswitch.MaterialSwitch
                            android:id="@+id/switchConcurrentGC"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="16dp" />

                    </LinearLayout>
                </LinearLayout>
            </com.google.android.material.card.MaterialCardView>

            <!-- Tiered Compilation -->
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/tieredCompilationCard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="12dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="2dp"
                app:strokeWidth="0.5dp"
                app:strokeColor="?attr/colorOutlineVariant"
                app:cardBackgroundColor="?attr/colorSurfaceContainer"
                style="@style/Widget.App.CardView.Standard">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingHorizontal="16dp"
                    android:paddingVertical="14dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center_vertical">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/coreclr_tiered_compilation"
                                android:textColor="?attr/textPrimary"
                                android:textSize="15sp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/coreclr_tiered_compilation_desc"
                                android:textColor="?attr/textSecondary"
                                android:textSize="12sp"
                                android:layout_marginTop="2dp" />

                        </LinearLayout>

                        <com.google.android.material.materialswitch.MaterialSwitch
                            android:id="@+id/switchTieredCompilation"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="16dp" />

                    </LinearLayout>
                </LinearLayout>
            </com.google.android.material.card.MaterialCardView>

            <!-- 调试选项 -->
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/settings_debug_options"
                android:textColor="?attr/textPrimary"
                android:textSize="16sp"
                android:textStyle="bold"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="12dp" />

            <!-- 启用日志系统卡片 -->
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/enableLogSystemCard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="12dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="2dp"
                app:strokeWidth="0.5dp"
                app:strokeColor="?attr/colorOutlineVariant"
                app:cardBackgroundColor="?attr/colorSurfaceContainer"
                style="@style/Widget.App.CardView.Standard">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingHorizontal="16dp"
                    android:paddingVertical="14dp">

                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:src="@drawable/ic_bug"
                        app:tint="?attr/textPrimary"
                        android:layout_marginEnd="16dp" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/enable_log_system"
                            android:textColor="?attr/textPrimary"
                            android:textSize="15sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/enable_log_system_desc"
                            android:textColor="?attr/textSecondary"
                            android:textSize="12sp"
                            android:layout_marginTop="2dp" />

                    </LinearLayout>

                    <com.google.android.material.materialswitch.MaterialSwitch
                        android:id="@+id/switchEnableLogSystem"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp" />

                </LinearLayout>

            </com.google.android.material.card.MaterialCardView>

            <!-- 详细日志卡片 -->
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/verboseLoggingCard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="12dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="2dp"
                app:strokeWidth="0.5dp"
                app:strokeColor="?attr/colorOutlineVariant"
                app:cardBackgroundColor="?attr/colorSurfaceContainer"
                style="@style/Widget.App.CardView.Standard">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingHorizontal="16dp"
                    android:paddingVertical="14dp">

                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:src="@drawable/ic_bug"
                        app:tint="?attr/textPrimary"
                        android:layout_marginEnd="16dp" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/verbose_logging"
                            android:textColor="?attr/textPrimary"
                            android:textSize="15sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/verbose_logging_desc_on"
                            android:textColor="?attr/textSecondary"
                            android:textSize="12sp"
                            android:layout_marginTop="2dp" />

                    </LinearLayout>

                    <com.google.android.material.materialswitch.MaterialSwitch
                        android:id="@+id/switchVerboseLogging"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp" />

                </LinearLayout>

            </com.google.android.material.card.MaterialCardView>

            <!-- 导出并分享日志文件卡片 -->
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/exportShareLogCard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="12dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="2dp"
                app:strokeWidth="0.5dp"
                app:strokeColor="?attr/colorOutlineVariant"
                app:cardBackgroundColor="?attr/colorSurfaceContainer"
                style="@style/Widget.App.CardView.Standard">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingHorizontal="16dp"
                    android:paddingVertical="14dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center_vertical">

                        <ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:src="@drawable/ic_export"
                            app:tint="?attr/textPrimary"
                            android:layout_marginEnd="16dp"
                            android:contentDescription="@string/export_share_log" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/export_share_log"
                                android:textColor="?attr/textPrimary"
                                android:textSize="15sp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/export_share_log_desc"
                                android:textColor="?attr/textSecondary"
                                android:textSize="12sp"
                                android:layout_marginTop="2dp" />

                        </LinearLayout>

                        <com.google.android.material.button.MaterialButton
                            android:id="@+id/btnExportShareLog"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/export"
                            android:layout_marginStart="16dp"
                            style="@style/Widget.Material3.Button.TonalButton" />

                    </LinearLayout>

                </LinearLayout>

            </com.google.android.material.card.MaterialCardView>

            <!-- 实验性功能 -->
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/settings_experimental_features"
                android:textColor="?attr/textPrimary"
                android:textSize="16sp"
                android:textStyle="bold"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="12dp" />

            <!-- 大核心线程亲和性卡片 -->
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/threadAffinityCard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="12dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="2dp"
                app:strokeWidth="0.5dp"
                app:strokeColor="?attr/colorOutlineVariant"
                app:cardBackgroundColor="?attr/colorSurfaceContainer"
                style="@style/Widget.App.CardView.Standard">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingHorizontal="16dp"
                    android:paddingVertical="14dp">

                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:src="@drawable/ic_cpu"
                        app:tint="?attr/textPrimary"
                        android:layout_marginEnd="16dp" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/thread_affinity_big_core"
                            android:textColor="?attr/textPrimary"
                            android:textSize="15sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/thread_affinity_big_core_desc"
                            android:textColor="?attr/textSecondary"
                            android:textSize="12sp"
                            android:layout_marginTop="2dp" />

                    </LinearLayout>

                    <com.google.android.material.materialswitch.MaterialSwitch
                        android:id="@+id/switchThreadAffinityToBigCore"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp" />

                </LinearLayout>

            </com.google.android.material.card.MaterialCardView>

            <!-- Kill Launcher UI to Save Memory -->
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/killLauncherUICard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="12dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="2dp"
                app:strokeWidth="0.5dp"
                app:strokeColor="?attr/colorOutlineVariant"
                app:cardBackgroundColor="?attr/colorSurfaceContainer"
                style="@style/Widget.App.CardView.Standard">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingHorizontal="16dp"
                    android:paddingVertical="14dp">

                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:src="@drawable/ic_memory"
                        app:tint="?attr/textPrimary"
                        android:layout_marginEnd="16dp" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/kill_launcher_ui_after_launch"
                            android:textColor="?attr/textPrimary"
                            android:textSize="15sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/kill_launcher_ui_after_launch_desc"
                            android:textColor="?attr/textSecondary"
                            android:textSize="12sp"
                            android:layout_marginTop="2dp" />

                    </LinearLayout>

                    <com.google.android.material.materialswitch.MaterialSwitch
                        android:id="@+id/switchKillLauncherUI"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp" />

                </LinearLayout>

            </com.google.android.material.card.MaterialCardView>

            <!-- FNA Map Buffer Range Optimization -->
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/fnaMapBufferRangeOptCard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="12dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="2dp"
                app:strokeWidth="0.5dp"
                app:strokeColor="?attr/colorOutlineVariant"
                app:cardBackgroundColor="?attr/colorSurfaceContainer"
                style="@style/Widget.App.CardView.Standard">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingHorizontal="16dp"
                    android:paddingVertical="14dp">

                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:src="@drawable/ic_fire"
                        app:tint="?attr/textPrimary"
                        android:layout_marginEnd="16dp"
                        android:contentDescription="@string/fna_map_buffer_range_opt" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/fna_map_buffer_range_opt"
                            android:textColor="?attr/textPrimary"
                            android:textSize="15sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/fna_map_buffer_range_opt_desc"
                            android:textColor="?attr/textSecondary"
                            android:textSize="12sp"
                            android:layout_marginTop="2dp" />

                    </LinearLayout>

                    <com.google.android.material.materialswitch.MaterialSwitch
                        android:id="@+id/switchFnaMapBufferRangeOpt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp" />


                </LinearLayout>

            </com.google.android.material.card.MaterialCardView>

            <!-- Low Latency Audio -->
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/lowLatencyAudioCard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="12dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="2dp"
                app:strokeWidth="0.5dp"
                app:strokeColor="?attr/colorOutlineVariant"
                app:cardBackgroundColor="?attr/colorSurfaceContainer"
                style="@style/Widget.App.CardView.Standard">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingHorizontal="16dp"
                    android:paddingVertical="14dp">

                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:src="@drawable/ic_audio"
                        app:tint="?attr/textPrimary"
                        android:layout_marginEnd="16dp"
                        android:contentDescription="@string/low_latency_audio" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/low_latency_audio"
                            android:textColor="?attr/textPrimary"
                            android:textSize="15sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/low_latency_audio_desc"
                            android:textColor="?attr/textSecondary"
                            android:textSize="12sp"
                            android:layout_marginTop="2dp" />

                    </LinearLayout>

                    <com.google.android.material.materialswitch.MaterialSwitch
                        android:id="@+id/switchLowLatencyAudio"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp" />

                </LinearLayout>

            </com.google.android.material.card.MaterialCardView>

            <!-- SteamCMD and Box64 removed -->

        </LinearLayout>

    </androidx.core.widget.NestedScrollView>

