{
  "id": "com.app.ralaunch.tmodloader.hostandplaypatch",
  "name": "Host & Play (多人服务器支持)",
  "description": "通过新的dotnet进程开启服务器",
  "version": "1.0.1",
  "author": "RAL",
  "dllFileName": "HostAndPlayPatch.dll",
  "entryPoint": {
    "typeName": "HostAndPlayPatch.StartupHook",
    "methodName": "Initialize"
  },
  "targetGames": [
    "tModLoader",
    "Terraria"
  ],
  "priority": 50,
  "enabled": true,
  "environmentVariables": {
    "HOSTANDPLAY_ENABLED": "1"
  },
  "features": [
    "支持本地回环TCP连接（可选）",
    "支持最多8个玩家（可配置）"
  ]
}
